<UserControl x:Class="Conv_TF_UI.Manual_Screen"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:Conv_TF_UI"
             mc:Ignorable="d"
             d:DesignHeight="490"
             d:DesignWidth="880">
    <Grid>
        <Grid Margin="0,10,0,0">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Top">
                <Button Content="Cụm 1" x:Name="bt_Cum1" Width="270"
Height="40" Margin="15,0,0,0" Click="bt_Cum1_Click"
/>
                <Button Content="Cụm 2" x:Name="bt_Cum2" Width="270"
Height="40" Margin="20,0,0,0" Click="bt_Cum2_Click"
/>
                <Button Content="Cụm Hoàn Thành" x:Name="bt_CumHoanThanh" Width="270" 
Height="40" Margin="20,0,0,0" Click="bt_CumHoanThanh_Click"
/>
            </StackPanel>
        </Grid>
        <StackPanel x:Name="Manual_Monitor" HorizontalAlignment="Center" VerticalAlignment="Center" />
    </Grid>
</UserControl>
